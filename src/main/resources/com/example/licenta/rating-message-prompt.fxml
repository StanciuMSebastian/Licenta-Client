<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<DialogPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.licenta.controllers.RatingMessagePromptController">
   <content>
      <AnchorPane prefHeight="242.0" prefWidth="572.0">
         <children>
            <Label layoutX="144.0" layoutY="14.0" prefHeight="55.0" prefWidth="284.0" text="Please enter a feedback message (max. 250 characters)" textAlignment="CENTER" wrapText="true" />
            <TextArea fx:id="ratingMessageField" layoutX="186.0" layoutY="69.0" prefHeight="100.0" prefWidth="200.0" wrapText="true" />
         </children>
      </AnchorPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="CANCEL" />
      <ButtonType fx:constant="OK" />
   </buttonTypes>
</DialogPane>
